{
  "name": "vscode-ime",
  "version": "0.0.1",
  "publisher": "kaiserzh",
  "engines": {
    "vscode": "^1.0.0"
  },
  "displayName": "vscode-ime",
  "description": "IME",
  "main": "./extension.js",
  "author": {
    "name": "Kaiser Zh"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-ime.toggle"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-ime.toggle",
        "title": "Toggle Google Pinyin"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-ime.toggle",
        "key": "ctrl+space"
      },
      {
        "command": "vscode-ime.select.source.commit",
        "key": "enter",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.select.candidate.commit",
        "key": "space",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.pageup",
        "key": "=",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.pageup",
        "key": ".",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.pagedown",
        "key": "-",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.pagedown",
        "key": ",",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.select.key.1",
        "key": "1",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.select.key.2",
        "key": "2",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.select.key.3",
        "key": "3",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.select.key.4",
        "key": "4",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.select.key.5",
        "key": "5",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.select.key.6",
        "key": "6",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.select.key.7",
        "key": "7",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.select.key.8",
        "key": "8",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.select.key.9",
        "key": "9",
