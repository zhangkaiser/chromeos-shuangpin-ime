      },
      {
        "command": "vscode-ime.select.key.0",
        "key": "0",
        "when": "vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.editor.keyA",
        "key": "a",
        "when": "editorTextFocus && vscode-ime.init && !vscode-ime.selecting"
      },
      {
        "command": "vscode-ime.editor.keyB",
        "key": "b",
