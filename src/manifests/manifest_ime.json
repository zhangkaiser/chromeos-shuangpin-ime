{
  "name": "IME",
  "version": "0.0.1",
  "description": "支持拼音加加,小鹤等解决方法.",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "input",
    "unlimitedStorage",
    "background",
    "storage"
  ],
  "host_permissions": [
    "https://olime.baidu.com/py?*",
    "https://www.google.com/inputtools/request?*",
    "https://www.google.cn/inputtools/request?*"
  ],
  "input_components": [
    {
      "name": "中文拼音输入法（带预测候选词）",
      "type": "ime",
      "id": "zh-wasm-pinyin",
      "description": "拼音输入法",
      "language": "zh",
      "layouts": ["us"],
      "options_page": "option.html?code=zh-wasm-pinyin"
      },
      {
        "name": "中文双拼输入法",
        "type": "ime",
        "id": "zh-wasm-shuangpin",
        "description": "",
        "language": "zh",
        "layouts": ["us"],
        "options_page": "option.html?code=zh-wasm-shuangpin"
      }
  ],
  "options_page": "option.html",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "key": ""
}