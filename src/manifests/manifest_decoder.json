{
  "name": "Decoder",
  "version": "0.0.1",
  "description": "支持拼音加加,小鹤等解决方法.",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "unlimitedStorage",
    "background",
    "storage"
  ],
  "host_permissions": [
    "https://olime.baidu.com/py?*",
    "https://www.google.com/inputtools/request?*",
    "https://www.google.cn/inputtools/request?*"
  ],
  "options_page": "option.html",
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval' 'wasm-eval'; object-src 'self'"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "key": ""
}